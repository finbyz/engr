 "doc_type": "Proforma Invoice",
 "docstatus": 0,
 "doctype": "Print Format",
 "font": "Default",
 "html": "<style>\r\n   .tbspace>tbody>tr>td {\r\n   padding: 0 2px 0 2px !important;\r\n   margin: 0 !important;\r\n   border-spacing: 0 !important;\r\n   }\r\n   .table-bordered>tbody>tr>td{\r\n   font-size: 8px;\r\n   padding: 0px !important;\r\n   }\r\n   div>u{\r\n   font-size: 9px;\r\n   }\r\n   .print-format {\r\n   margin-top: 0mm !important;\r\n   margin-bottom: 0mm !important;\r\n   margin-left: 0mm !important;\r\n   margin-right: 0mm !important;\r\n   }\r\n   table {\r\n   border-collapse: collapse;\r\n   border-spacing: 00 !important;\r\n   }\r\n   .ontop {\r\n   border-top: 1px solid !important;\r\n   }\r\n   .onright {\r\n   border-right: 1px solid !important;\r\n   }\r\n   .onleft {\r\n   border-left: 1px solid !important;\r\n   }\r\n   .onbottom {\r\n   border-bottom: 1px solid !important;\r\n   }\r\n   .onleftright {\r\n   border-right: 1px solid !important;\r\n   border-left: 1px solid !important;\r\n   }\r\n   .noright {\r\n   border-right: 0 !important;\r\n   }\r\n   .noleft {\r\n   border-left: 0 !important;\r\n   }\r\n   .noleftright {\r\n   border-right: 0 !important;\r\n   border-left: 0 !important;\r\n   }\r\n   .notop {\r\n   border-top: 0 !important;\r\n   }\r\n   .nobottom {\r\n   border-bottom: 0 !important;\r\n   }\r\n   .notopbottom {\r\n   border-top: 0 !important;\r\n   border-bottom: 0 !important;\r\n   }\r\n   td.v-align {\r\n   vertical-align: middle !important;\r\n   }\r\n   .bg {\r\n   background-color: #d2d2d2;\r\n   }\r\n   td.v-align-bt {\r\n   vertical-align: bottom !important;\r\n   }\r\n   .text-size {\r\n   font-size: 9px !important;\r\n   line-height: 1.1;\r\n   }\r\n   .sqare {\r\n   font-size: 20px;\r\n   }\r\n   .t-table {\r\n   border: 1px solid black;\r\n   height: 180px;\r\n   border-collapse: collapse;\r\n   }\r\n   .t-table th {\r\n   border: 1px solid black;\r\n   height: 10px;\r\n   }\r\n   .border-bottom {\r\n   border-bottom: 1px solid black;\r\n   }\r\n   .t-table td {\r\n   border-left: 1px solid black;\r\n   border-right: 1px solid black;\r\n   }\r\n   .border-table tr:last-child {\r\n   border-bottom: 1px solid black;\r\n   }\r\n   .description-font * {\r\n   font-size: 9px !important;\r\n   }\r\n</style>\r\n<div id=\"header-html\" class=\"hidden-pdf\">\r\n   {% if letter_head and not no_letterhead %}\r\n   <div class=\"letter-head\">{{ letter_head }}</div>\r\n   {% endif %}\r\n</div>\r\n<br>\r\n<div style=\"padding-left:55px !important; padding-right: 10px !important;\">\r\n<table border=\"0\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace\">\r\n   <tbody>\r\n      <tr>\r\n         <td width=\"32%\" class=\"v-align ontop onleft onright\">\r\n            <p class=\"text-size\">\r\n               <strong>\r\n               {{ doc.company }}\r\n               </strong>\r\n            </p>\r\n            <p class=\"text-size\">\r\n               <strong>\r\n               </strong>\r\n            </p>\r\n         </td>\r\n         <td width=\"78%\" class=\"v-align ontop onleft onright\" colspan=\"3\">\r\n            <p align=\"center\" class=\"text-size\"><strong>PROFORMA INVOICE</strong></p>\r\n         </td>\r\n      </tr>\r\n      <tr>\r\n         <td width=\"32%\" rowspan=\"3\" class=\"onleftright\">\r\n            <p class=\"text-size\">\r\n               {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line1') or \"\" }}\r\n            </p>\r\n            {%- if frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line2') -%}\r\n            <p class=\"text-size\">\r\n               {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'address_line2') or \"\" }}\r\n            </p>\r\n            {%- endif -%}\r\n            <p class=\"text-size\">\r\n               {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'city') or \"\" }}-\r\n               {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'pincode') or \"\" }},\r\n            </p>\r\n            <p class=\"text-size\">\r\n               {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'state') or \"\" }}\r\n               ,{{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'country')|upper or \"\" }}\r\n            </p>\r\n            {% if frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'gstin') %}\r\n            <p class=\"text-size\">\r\n               GSTIN : {{ frappe.db.get_value(\"Address\",{ 'name':doc.company_address},'gstin') or \"\" }}\r\n            </p>\r\n            {% endif %}\r\n            {% if frappe.db.get_value(\"Company\",doc.company,'tax_id') %}\r\n            <p class=\"text-size\">\r\n               PAN   :  {{ frappe.db.get_value(\"Company\",doc.company,'tax_id') or ''}}\r\n            </p>\r\n            {% endif %}\r\n            \r\n         </td>\r\n         <td width=\"20%\" class=\"ontop onright\">\r\n            <p class=\"text-size\">\r\n               Date :\r\n            </p>\r\n            <p class=\"text-size\">\r\n               <strong>{{ frappe.utils.formatdate(frappe.utils.now().split(\" \") [0], \"dd-MM-yyyy\") or ''  }}&nbsp;&nbsp;&nbsp;</strong>\r\n            </p>\r\n         </td>\r\n         <td  width=\"23%\" class=\"ontop onright\">\r\n            <p class=\"text-size\">\r\n               Order No. : <strong>{{ doc.items[0].sales_order or doc.sales_order or ''}}</strong>\r\n            </p>\r\n            <p class=\"text-size\">\r\n               Dated :  <strong>{{ frappe.utils.formatdate(frappe.db.get_value(\"Sales Order\",doc.items[0].sales_order,'transaction_date'), \"dd-MM-yyyy\") or '' }}</strong>\r\n            </p>\r\n         </td>\r\n         <td class=\"ontop onright\" width=\"23%\">\r\n            <p class=\"text-size\">\r\n               <span >Proforma Invoice No.</span><br>\r\n               <strong>{{ doc.name or ''  }}</strong>\r\n            </p>\r\n            <p class=\"text-size\">\r\n               <span class=\"text-size\">Dated :</span>\r\n               <strong>{{ doc.get_formatted(\"transaction_date\") }}</strong>\r\n            </p>\r\n         </td>\r\n      </tr>\r\n      <tr>\r\n         <td class=\"ontop onright\">\r\n            \r\n            <p class=\"text-size\">\r\n               Vehicle No. : <strong>{{ doc.vehicle_no or '' }}</strong>\r\n            </p>\r\n         </td>\r\n         <td class=\"ontop onright\">\r\n            \r\n            <p class=\"text-size\">\r\n               Job Number :<strong> {% if doc.job_id %}{{ (doc.job_id)[0:6] or \"\"}}{% endif %}</strong>\r\n            </p>\r\n            \r\n            {% set wo_master_id = frappe.db.exists(\"Work Order Master\",{\"job_id\":doc.job_id }) %}\r\n            <p class=\"text-size\">WOM :- {{ wo_master_id or \"\"}} </p>\r\n         </td>\r\n         <td class=\"ontop onright\">\r\n            <p class=\"text-size\">\r\n              State:\r\n               <strong> {{ frappe.db.get_value(\"Address\",doc.compant_address ,\"state\") or \"\"}}</strong>\r\n            </p>\r\n            <p class=\"text-size\">Code:{{ frappe.db.get_value(\"Address\",doc.compant_address ,\"gst_state_number\") or \"\"}}</p>\r\n         </td>\r\n      </tr>\r\n      <tr>\r\n         <td width=\"25%\" class=\"ontop onright\" colspan=\"2\">\r\n            <p class=\"text-size\">\r\n               Payment Terms :<strong>{{ doc.payment_terms_template or '' }}</strong>  \r\n            </p>\r\n         </td>\r\n         <td width=\"25%\" class=\"ontop onright\" colspan=\"2\">\r\n            <p class=\"text-size\">\r\n                Place of Supply :<strong><br> {{ doc.place_of_supply or ''}}</strong>\r\n            </p>\r\n         </td>\r\n      </tr>\r\n   </tbody>\r\n</table>\r\n<table border=\"0\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace\">\r\n<tbody>\r\n   <tr>\r\n      <td width=\"50%\" class=\"ontop onleftright\">\r\n         <p class=\"text-size\">\r\n            <u>Name &amp; Address of Buyer</u>\r\n         </p>\r\n         <p class=\"text-size\">\r\n            <strong>{{ doc.customer_name|upper }}</strong>\r\n         </p>\r\n         {% set address = frappe.db.get_value(\"Address\", doc.customer_address ,['address_line1', 'address_line2', 'city', 'pincode', 'state' ,'gstin' ,'gst_state_number']) %}\r\n         {%- if address -%}\r\n         <p class=\"text-size\">\r\n            {{ address[0]|upper if address[0] else \"\" }}\r\n         </p>\r\n         <p class=\"text-size\">\r\n            {{ address[1]|upper if address[1] else \"\" }}\r\n         </p>\r\n         <p class=\"text-size\">\r\n            {{ address[2]|upper if address[2] else \"\" }}-\r\n            {{ address[3]|upper if address[3] else \"\" }},\r\n         </p>\r\n         <p class=\"text-size\">\r\n            {{ address[4]|upper if address[4] else \"\" }}\r\n         </p>\r\n         {%- endif -%}\r\n         {% if frappe.db.get_value(\"Customer\",doc.customer,'tax_id') %}\r\n         <p class=\"text-size\">\r\n            PAN : {{ frappe.db.get_value(\"Customer\",doc.customer,'tax_id') or ''}}\r\n         </p>\r\n         {% endif %}\r\n         {% if address[5] %}\r\n         <p class=\"text-size\">\r\n            GSTIN NO. {{ address[5]|upper if address[5] else \"\" }} &nbsp;&nbsp;&nbsp; STATE CODE\r\n            {{ address[6]|upper if address[6] else \"\" }}\r\n         </p>\r\n         {% endif %}\r\n      </td>\r\n      <td width=\"50%\" class=\"ontop onright\">\r\n         <p class=\"text-size\">\r\n            <u>Name &amp; Address of Consignee </u>\r\n         </p>\r\n         <p class=\"text-size\">\r\n            <strong>\r\n            {{  doc.customer_name|upper or '' }}\r\n            </strong>\r\n         </p>\r\n         <p class=\"text-size\">\r\n            {{ doc.shipping_address or '' }}\r\n         </p>\r\n      </td>\r\n   </tr>\r\n</tbody>\r\n</table>\r\n<!--<table class=\"tbspace\">-->\r\n   <!--<tr class=\"onleft onright ontop\">-->\r\n   <!--       <td width=\"40%\" >-->\r\n   <!--           <p class=\"text-size\">Ref  Letter:</p>\t\t\t\t\t\t\t-->\r\n   <!--           <strong class=\"text-size\">L.NO.SIPL/NMSCDCL/GRF/35/2022  Dated 23/04/2022</strong><br>-->\r\n   <!--       </td>-->\r\n   <!--       <td class=\"onleft \">-->\r\n   <!--         <p class=\"text-size\"> P.O No/Site Details:\t </p>-->\r\n   <!--           <strong class=\"text-size\">-->\r\n   <!--           \"Construction of Major Bridge on Godavari Riverfront Development including beautification and infrastructure components in ABD area under Nashik Smart City Mission, Nashik\"-->\r\n   <!--             </strong>-->\r\n   <!--       </td>-->\r\n<!--      </tr>-->\r\n   <!--</table>-->\r\n{% set length = doc.items|length %} {% set ln = 31 * doc.items|length %} {% set def = 160 %}\r\n<table border=\"0\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"t-table tbspace\" style=\"min-height: {{ ([ln,def]|max) }}px!important;\">\r\n   <tbody>\r\n      <tr class=\"border-bottom\" style=\"height: 10px;\">\r\n         <td width=\"4%\" class=\"ontop onleftright v-align\">\r\n            <p align=\"center\" class=\"text-size\">\r\n               <strong>Sr. No.</strong>\r\n            </p>\r\n         </td>\r\n         <td width=\"35%\" class=\"ontop onright v-align\">\r\n            <p align=\"center\" class=\"text-size\">\r\n               <strong>Items</strong>\r\n            </p>\r\n         </td>\r\n         <td width=\"10%\" class=\"ontop onright v-align\">\r\n            <p align=\"center\" class=\"text-size\">\r\n               <strong>HSN Code</strong>\r\n            </p>\r\n         </td>\r\n         <td width=\"10%\" class=\"ontop onright v-align\">\r\n            <p align=\"center\" class=\"text-size\">\r\n               <strong>Quantity</strong>\r\n            </p>\r\n         </td>\r\n         {% if doc.discount %}\r\n          <td width=\"10%\" class=\"ontop onright v-align\">\r\n            <p align=\"center\" class=\"text-size\">\r\n               <strong>Discount</strong>\r\n            </p>\r\n         </td>\r\n         {% endif %}\r\n         <td width=\"10%\" class=\"ontop onright v-align\">\r\n            <p align=\"center\" class=\"text-size\">\r\n               <strong>Rate</strong>\r\n            </p>\r\n         </td>\r\n         <td width=\"14%\" class=\"ontop onright v-align\">\r\n            <p align=\"center\" class=\"text-size\">\r\n               <strong> Total</strong>\r\n            </p>\r\n         </td>\r\n      </tr>\r\n      {% for row in doc.items %}\r\n      <tr style=\"height: 10px;\">\r\n         <td  class=\"notopbottom onleftright\">\r\n            <p align=\"center\" class=\"text-size\">\r\n               {{ row.idx }}\r\n            </p>\r\n         </td>\r\n         <td  class=\"notopbottom onright text-size\">\r\n            <p align=\"left\" class=\"text-size\" >\r\n               {{ row.item_name }}\r\n            </p>\r\n            <p align=\"left\" class=\"text-size\" >\r\n               {% if row.item_name != row.item_code %}\r\n               {{ row.item_code or '' }}\r\n               {% endif %}\r\n            </p>\r\n            {% if row.item_name != row.description %}\r\n            <p align=\"left\" class=\"text-size\">\r\n               {{ row.description or '' }}\r\n            </p>\r\n            {% endif %}\r\n         </td>\r\n         <td  class=\"notopbottom onright\">\r\n            <p align=\"center\" class=\"text-size\">\r\n               {{ row.gst_hsn_code or '' }}\r\n            </p>\r\n         </td>\r\n         <td class=\"notopbottom onleftright\">\r\n            <p align=\"center\" class=\"text-size\">\r\n               {% if doc.is_return %} {{ \"%.2f\"|format(row.qty * -1) }} {% else %} {{ \"%.2f\"|format(row.qty) }} {% endif %}{{ row.stock_uom or ''}}\r\n            </p>\r\n         </td>\r\n         {% if doc.discount %}\r\n         <td class=\"notopbottom onleftright\">\r\n            <p align=\"center\" class=\"text-size\">\r\n              {{ row.discount_percentage or \"\" }}{% if row.discount_percentage %}%{% endif %}\r\n            </p>\r\n         </td>\r\n         {% endif %}\r\n         <td class=\"notopbottom onleftright\">\r\n            <p align=\"center\" class=\"text-size\">\r\n               {{ row.get_formatted(\"rate\") }}\r\n            </p>\r\n         </td>\r\n         <td  class=\"notopbottom\">\r\n            <p align=\"right\" class=\"text-size\">\r\n               {% if doc.is_return %}\r\n               {{ frappe.db.get_value(\"Currency\",doc.currency,\"symbol\") or \"\" }}{{ row.amount * -1 }} {% else %}\r\n               {{ row.get_formatted('amount') }} {% endif %}\r\n            </p>\r\n         </td>\r\n      </tr>\r\n      {% endfor %}\r\n      <tr>\r\n         <td class=\"onright\">\r\n         </td>\r\n         <td class=\"onright\">\r\n         </td>\r\n         <td class=\"onright\">\r\n         </td>\r\n         <td class=\"onright\">\r\n         </td>\r\n         \r\n         <td class=\"onright\">\r\n         </td>\r\n         <td class=\"onright\">\r\n         </td>\r\n      </tr>\r\n      <tr style=\"height: 10px;\">\r\n         <td colspan=\"3\" class=\"ontop onleftright\">\r\n            <p class=\"text-size\" align=\"right\">\r\n               Total\r\n            </p>\r\n         </td>\r\n         <td class=\"ontop onright\">\r\n            <p align=\"center\" class=\"text-size\">\r\n               <strong>{% if doc.is_return %} {{ \"%.3f\"|format(doc.total_qty * -1) or '' }} {% else %} {{ \"%.3f\"|format(doc.total_qty) or '' }}\r\n               {% endif %}</strong>\r\n            </p>\r\n         </td>\r\n         {% if doc.discount %}\r\n         <td class=\"ontop onright\">\r\n            <p align=\"center\" class=\"text-size\">\r\n            </p>\r\n         </td>\r\n         {% endif %}\r\n         <td class=\"ontop onright\">\r\n            <p align=\"center\" class=\"text-size\">\r\n            </p>\r\n         </td>\r\n         <td class=\"ontop onright\">\r\n            <p align=\"right\" class=\"text-size\">\r\n               <strong>\r\n               {% if doc.is_return %}\r\n               {{ frappe.db.get_value(\"Currency\",doc.currency,\"symbol\") or \"\" }}{{ doc.total * -1 }} {% else %}\r\n               {{ doc.get_formatted('total') }} {% endif %}</strong>\r\n            </p>\r\n         </td>\r\n      </tr>\r\n   </tbody>\r\n</table>\r\n\r\n<table border=\"1\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace\">\r\n   <tbody>\r\n      <tr>\r\n         <td width=\"60%\" >\r\n            <p class=\"text-size\" align=\"left\">\r\n               <strong> (-) DISCOUNT</strong>\r\n            </p>\r\n         </td>\r\n         <td>\r\n            <p class=\"text-size\" align=\"center\">\r\n               {{ doc.additional_discount_percentage or \"\"}}%\r\n            </p>\r\n         </td>\r\n         <td class=\"text-size\">\r\n            <p class=\"text-size\" align=\"right\">\r\n               {% if doc.is_return %}\r\n               {{ frappe.db.get_value(\"Currency\",doc.currency,\"symbol\") or \"\" }}{{ doc.discount_amount * -1 }}\r\n               {% else %} {{ doc.get_formatted('discount_amount') }} {% endif %}\r\n            </p>\r\n         </td>\r\n      </tr>\r\n      {% for row in doc.taxes %}\r\n      <tr>\r\n         <td  align=\"left\">\r\n            <p class=\"text-size\">\r\n               <strong>{{ row.description }}</strong>\r\n            </p>\r\n         </td>\r\n         <td>\r\n            <p class=\"text-size\">\r\n            </p>\r\n         </td>\r\n         <td>\r\n            <p class=\"text-size\" align=\"right\">\r\n               <strong>\r\n               {% if doc.is_return %}\r\n               {{ frappe.db.get_value(\"Currency\",doc.currency,\"symbol\") or \"\" }}{{ row.tax_amount * -1 }}\r\n               {% else %} {{ row.get_formatted('tax_amount') }} {% endif %}\r\n               </strong>\r\n            </p>\r\n         </td>\r\n      </tr>\r\n      {% endfor %}\r\n      <tr>\r\n         <td colspan=\"2\" class=\"ontop v-align-bt\">\r\n            <p align=\"left\" class=\"text-size\">\r\n               <strong>Invoice Total</strong>\r\n            </p>\r\n         </td>\r\n         \r\n         <td class=\"ontop v-align-bt\">\r\n            <p align=\"right\" style=\"font-size: 10px;\">\r\n               <strong>{% if doc.is_return %}\r\n               {{ frappe.db.get_value(\"Currency\",doc.currency,\"symbol\") or \"\" }}\r\n               {{ doc.rounded_total * -1 }} {% else %}\r\n               {% if doc.rounded_total %}{{ frappe.format_value(doc.rounded_total|int, \"Currency\", currency=doc.currency) }}{% else%}{{ frappe.format_value(doc.grand_total|int, \"Currency\", currency=doc.currency) }}{%endif%}\r\n               {% endif %}</strong>\r\n               <!--{{doc.get_formatted('rounded_total')}} -->\r\n               <!--{{doc.get_formatted('grand_total')}}-->\r\n            </p>\r\n         </td>\r\n      </tr>\r\n   </tbody>\r\n</table>\r\n<table border=\"0\" cellspacing=\"0\" cellpadding=\"0\" width=\"100%\" class=\"tbspace\">\r\n   <tbody>\r\n      <tr>\r\n         <td width=\"35%\"  class=\" onleft \">\r\n            <p class=\"text-size\">\r\n               <b>Bank Details :</b>\r\n            </p>\r\n            <p class=\"text-size\">\r\n               <strong> BANK NAME</strong>\r\n               : {{ frappe.db.get_value(\"Bank Account\",{ 'name':doc.bank_account},'bank') or \"\" }}\r\n            </p>\r\n            <p class=\"text-size\"><strong>Bank Branch</strong>Bank Of  India.</p>\r\n            <p class=\"text-size\">\r\n               <strong> Branch code</strong>\r\n               : {{ frappe.db.get_value(\"Bank Account\",doc.bank_account,'branch_code') or '' }}\r\n            </p>\r\n            <p class=\"text-size\">\r\n               <strong> IFSC CODE</strong>\r\n               : {{ frappe.db.get_value(\"Bank Account\",doc.bank_account,'ifs_code') or '' }}\r\n            </p>\r\n            <p class=\"text-size\">\r\n               <strong>GST No</strong>\r\n               : {{ frappe.db.get_value(\"Address\",doc.company_address,'gstin') or '' }}\r\n            </p>\r\n            <p class=\"text-size\">\r\n               <strong>Pan No</strong>\r\n               : {{ frappe.db.get_value(\"Company\", doc.comapny, \"tax_id\") or ''}}\r\n            </p>\r\n            <p class=\"text-size\">\r\n               <strong>UPI Id</strong>\r\n               : {{ frappe.db.get_value(\"Bank\", frappe.db.get_value(\"Bank Account\", doc.bank_account, \"bank\"), \"upi_id\") or ''}}\r\n            </p>\r\n            <p class=\"text-size\">\r\n               <strong>  ACCOUNT NO</strong>\r\n               : {{ frappe.db.get_value(\"Bank Account\",doc.bank_account,'bank_account_no') or '' }}\r\n            </p>\r\n         </td>\r\n         <td class=\"onleft ontop onbottom\">\r\n             <img src=\"/files/barcode.png\" height=\"130px\" />\r\n         </td>\r\n         <td  class=\"onleft onright \" width=\"45%\">\r\n            {% if doc.payment_percentage < 100 %}\r\n            <p class=\"text-size\"><strong>Payment Percentage</strong>&nbsp;&nbsp;&nbsp;:&nbsp;{{ doc.get_formatted('payment_percentage')}}</p>\r\n            <p class=\"text-size\"><strong>Payment Due Amount</strong>&nbsp;:&nbsp;{{ doc.get_formatted('payment_due_amount')}}</p>\r\n            {% endif %}\r\n            <p class=\"text-size\">\r\n               Amount Chargeable (In Words)\r\n            </p>\r\n            <p class=\"text-size\">\r\n               <strong>\r\n                {{ frappe.utils.money_in_words(doc.rounded_total or doc.grand_total, doc.currency) }} \r\n               </strong>\r\n            </p>\r\n         </td>\r\n      </tr>\r\n      <!--<tr>-->\r\n      <!--   {{doc.other_charges_calculation or ''}}-->\r\n      <!--</tr>-->\r\n      <!--<tr>-->\r\n      <!--   {% if doc.terms %}-->\r\n      <!--   <td class=\"onright ontop onleft\" colspan=\"3\">-->\r\n     